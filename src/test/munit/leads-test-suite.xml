			<munit-tools:assert-equals doc:name="Assert equals" doc:id="9c7ugl2g-scfd-jjje-lehu-qurdvjh76vk" actual="#[payload]" expected='#[{
  success: payload.success,
  id: payload.id,
  message: payload.message,
  errors: payload.errors
}]'/>
		</munit:validation>
	</munit:test>
